{
	"compilerOptions": {
	  "target": "es6",
	  "module": "commonjs",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "baseUrl": ".",
	  "paths": {
		"$lib/*": ["src/lib/*"]
	  }
	},
	"include": ["src/**/*"],
	"exclude": ["node_modules"]
  }
  